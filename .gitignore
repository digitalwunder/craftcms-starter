# CRAFT ENVIRONMENT
.env.php
.env.sh
.env

# COMPOSER
/vendor

# CRAFT STORAGE
/craft/storage/*
/storage/*

# ASSETS
/web/assets/css
/web/assets/js
/web/assets/images

# IMAGER TRANSFORMS
/public/imager/*
/web/imager/*

# BUILD FILES
/bower_components
/node_modules
/build/*
/yarn-error.log

# MISC FILES
.cache
.DS_Store
.idea
.project
.settings
*.esproj
*.sublime-workspace
*.sublime-project
*.tmproj
*.tmproject
.vscode/*
!.vscode/settings.json
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/extensions.json
config.codekit3
prepros-6.config
