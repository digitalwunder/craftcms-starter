{
  "require": {
    "craftcms/cms": "3.4.27",
    "vlucas/phpdotenv": "^2.4.0",
    "superbig/craft3-mobiledetect": "1.0.2",
    "craftcms/redactor": "2.6.1",
    "ether/seo": "3.6.6",
    "verbb/super-table": "2.5.1",
    "sebastianlenz/linkfield": "1.0.23",
    "doublesecretagency/craft-siteswitcher": "2.1.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
